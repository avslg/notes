{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-var": "off",
        "no-plusplus": "off",
        "no-loop-func": "off",
        "prefer-const": "off" 
    }
}